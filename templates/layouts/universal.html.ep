  <!-- STYLE !-->
  <style>
	header {
	    color:#FBF8EF;
	    line-height:100px;
	    text-align:center;
	    font-size:30px;
	    background-color:#3B0B0B;
	    height:100px;
	    font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif; 
	}
	#content {
	    min-height: 610px;
	    text-align:left;
	    max-width: 670px;
	    font-family: Calibri,Candara,Segoe,Segoe UI,Optima,Arial,sans-serif; 
	}
	#single {
	    font-size: 1.5em;
	}
